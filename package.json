{
  "name": "bdo-stopwatch",
  "version": "1.0.0",
  "main": "./electron-main.js",
  "description": "stopwatch app for MMORPG black desert online",
  "author": {
    "name": "Commander386"
  },
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start": "electron .",
    "build:once": "ng build --base-href ./",
    "build:watch": "ng build --base-href ./ --watch",
    "package": "electron-builder -- --base-href ./",
    "postinstall": "electron-builder install-app-deps",
    "rebuild": "electron-rebuild -f -w sqlite3"
  },
  "build": {
    "appId": "commander386.bdo-stopwatch",
    "productName": "BDO-Stopwatch",
    "asar": true,
    "asarUnpack": "**\\*.{node,dll}",
    "files": [
      "dist/**/*",
      "package.json",
      "electron-main.js"
    ],
    "win": {
      "target": [
        "nsis"
      ],
      "publish": [
        {
          "provider": "github",
          "repo": "BDO_Stopwatch",
          "owner": "StephenDevenney",
          "host": "github.com",
          "protocol": "https",
          "releaseType": "draft",
          "private": false
        }
      ]
    },
    "directories": {
      "output": "dist"
    },
    "nsis": {
      "oneClick": false
    },
    "extraResources": [
      {
        "from": "src/assets/data",
        "to": "dist/assets/data",
        "filter": "database*"
      }
    ]
  },
  "dependencies": {
    "@angular-builders/custom-webpack": "^11.1.1",
    "@angular-devkit/build-angular": "^0.1102.12",
    "@angular-eslint/builder": "0.8.0-beta.6",
    "@angular-eslint/eslint-plugin": "0.8.0-beta.6",
    "@angular-eslint/eslint-plugin-template": "0.8.0-beta.6",
    "@angular-eslint/schematics": "^4.2.0",
    "@angular-eslint/template-parser": "0.8.0-beta.6",
    "@angular/animations": "~11.0.5",
    "@angular/cdk": "^11.2.9",
    "@angular/cli": "~11.0.5",
    "@angular/common": "~11.0.5",
    "@angular/compiler": "~11.0.5",
    "@angular/compiler-cli": "~11.0.5",
    "@angular/core": "~11.0.5",
    "@angular/forms": "~11.0.5",
    "@angular/language-service": "~11.0.5",
    "@angular/platform-browser": "~11.0.5",
    "@angular/platform-browser-dynamic": "~11.0.5",
    "@angular/router": "~11.0.5",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^14.14.41",
    "@types/sqlite3": "^3.1.7",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "codelyzer": "^6.0.0",
    "dotenv": "^9.0.2",
    "electron-reload": "^1.5.0",
    "electron-updater": "^4.3.9",
    "electron-window-state": "^5.0.3",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-electron": "^6.3.1",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "moment": "^2.29.1",
    "ngx-ui-loader": "^10.0.0",
    "popper.js": "^1.16.1",
    "postcss": "^8.2.15",
    "primeicons": "^4.1.0",
    "primeng": "^11.3.2",
    "protractor": "~7.0.0",
    "rxjs": "~6.6.0",
    "sqlite3": "^5.0.2",
    "ts-node": "~8.3.0",
    "tslib": "^2.0.0",
    "typescript": "~4.0.2",
    "webpack-dev-server": "^3.11.2",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "electron": "^12.0.4",
    "electron-builder": "^22.10.5"
  }
}
